{
  "name": "dash-comp",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "dash-comp/src",
  "prefix": "dash",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "dash-comp/ng-package.json",
        "tailwindConfig": "dash-comp/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "dash-comp/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "dash-comp/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "dash-comp/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["dash-comp/**/*.ts", "dash-comp/**/*.html"]
      }
    }
  },
  "tags": []
}
